{
	"name": "remix-template-netlify",
	"private": true,
	"description": "",
	"license": "",
	"sideEffects": false,
	"scripts": {
		"build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
		"dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
		"start": "cross-env NODE_ENV=production netlify dev",
		"lint": "eslint ./app/**/*.{js,tsx,jsx} --fix",
		"prettier": "prettier --write ./app",
		"production": "npm run build:css && remix build",
		"development": "cross-env NODE_ENV=development concurrently \"npm run dev:css\" \"netlify dev\""
	},
	"dependencies": {
		"@netlify/functions": "^0.10.0",
		"@remix-run/netlify": "^1.4.3",
		"@remix-run/node": "^1.4.3",
		"@remix-run/react": "^1.4.3",
		"cross-env": "^7.0.3",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"@remix-run/dev": "^1.4.3",
		"@remix-run/eslint-config": "^1.4.3",
		"@types/react": "^17.0.24",
		"@types/react-dom": "^17.0.9",
		"autoprefixer": "^10.4.5",
		"concurrently": "^7.1.0",
		"eslint": "^8.11.0",
		"postcss": "^8.4.13",
		"tailwindcss": "^3.0.24",
		"typescript": "^4.5.5",
		"eslint-config-airbnb": "^19.0.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.27.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"eslint-plugin-storybook": "^0.5.11",
		"eslint-webpack-plugin": "^3.0.1",
		"prettier": "^2.5.1",
		"prettier-plugin-tailwindcss": "^0.1.8"
	},
	"engines": {
		"node": ">=14"
	}
}
